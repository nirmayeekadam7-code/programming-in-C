#include <stdio.h>

int main() {
int n, i, j;
	    int matrix[100][100];
	scanf("%d", &n);

	for (i = 0; i < n; i++){
		for(j = 0; j < n; j++) {
			   scanf("%d", &matrix[i][j]);
		}
	}
	for (i = 0; i < n; i++){
		for (j = 0; j < n; j++){
			printf("%d ", matrix[j][i]);
		}
		printf("\n");
	}

    
    
    
    
	return 0;
}
